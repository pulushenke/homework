    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Launch firefox localhost ",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5500/",
            "webRoot": "${workspaceFolder}",
            "firefoxExecutable":"C:/Program Files/Firefox Developer Edition/firefox.exe"
        },
        {
            "name": "Launch index.html",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "file": "${workspaceFolder}/index.html",
            "firefoxExecutable":"C:/Program Files/Firefox Developer Edition/firefox.exe"
        }

    ]
}